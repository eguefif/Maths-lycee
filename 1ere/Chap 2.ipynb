   "source": [
    "def ft_abs(x):\n",
    "    if x < 0:\n",
    "        x = -x\n",
    "    return x\n",
    "\n",
    "x = np.arange(-10, 10, 0.1)\n",
    "y = np.array([ft_abs(a) for a in x])\n",
    "\n",
    "plt.plot(x, y)"
